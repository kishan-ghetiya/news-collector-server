{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:security/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "security",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "security/detect-object-injection": "off"
  },
  "ignorePatterns": ["node_modules", "build/", "target/"]
}
